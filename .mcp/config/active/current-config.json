{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "C:\\Users\\ormus"]
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"]
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "***CONFIGURED***"
      }
    },
    "puppeteer": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-puppeteer"]
    },
    "telegram": {
      "command": "npx",
      "args": ["-y", "telegram-mcp"],
      "env": {
        "TG_APP_ID": "***CONFIGURED***",
        "TG_API_HASH": "***CONFIGURED***"
      }
    },
    "whatsapp": {
      "command": "npx",
      "args": ["-y", "whatsapp-mcp-server"],
      "env": {}
    },
    "n8n": {
      "command": "npx",
      "args": ["-y", "n8n-mcp"],
      "env": {
        "N8N_API_KEY": "***CONFIGURED***",
        "N8N_BASE_URL": "https://ormus.app.n8n.cloud"
      }
    },
    "discord": {
      "command": "npx",
      "args": ["-y", "mcp-discord"],
      "env": {
        "DISCORD_BOT_TOKEN": "***CONFIGURED***"
      }
    },
    "supabase": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-supabase"],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "NEEDS_CONFIGURATION",
        "SUPABASE_PROJECT_ID": "OPTIONAL_PER_PROJECT"
      }
    }
  }
}
